     procedure_id : 1776039234027139915
     type_code : 14
     code : |1+
      // Résumé : Ajoute une ligne de texte dans l'élément SAI_STATUS
      // Syntaxe :
      // AjouteLigneDansStatus (<sLigneAAjouter> est chaîne)
      //
      // Paramètres :
      //	sLigneAAjouter (chaîne UNICODE) : Le texte à ajouter au composant de Status
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // AjouteLigneDansStatus("Je suis un texte à ajouter")
      //
      PROCEDURE AjouteLigneDansStatus(sLigneAAjouter est une chaîne) <thread principal>
      
      gsStatusTexte += sLigneAAjouter + RC
      SAI_Status = gsStatusTexte
      SAI_Status..Curseur = Taille(SAI_Status)+1
