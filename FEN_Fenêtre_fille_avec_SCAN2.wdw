      // Résumé : Procédure exécutée lorsque la fenêtre reçoit un résultat de scan
      // Syntaxe :
      //CB_TraiteScan (<sData> est chaîne, <sSymbology> est chaîne)
      //
      // Paramètres :
      //	sData (chaîne UNICODE) : Le contenu de l'élément scanné
      //	sSymbology (chaîne UNICODE) : La symbologie de l'élément scanné (ex: PDF128, QRCODE, EAN13,...)
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE CB_TraiteScan(sData est une chaine, sSymbology est une chaine)
      AjouteLigneDansStatus("Donnée scannée: " + sData)
      AjouteLigneDansStatus("Symbology: " + sSymbology)
      
     type : 458752
   -
     name : CB_Erreur
     procedure_id : 1776039234027008843
     type_code : 14
     code : |1+
      // Résumé : Procédure exécutée lorsque qu'une méthode a rencontré une erreur 
      // Syntaxe :
      // CB_Erreur (<sNomDuProfil> est chaîne, <sMessage> est chaîne, <sErreur> est chaîne)
      //
      // Paramètres :
      //	sNomDuProfil (chaîne UNICODE) : Le nom du profil concerné par l'erreur
      //	sMessage (chaîne UNICODE) : Un message associé à l'erreur
      //	sErreur (chaîne UNICODE) : Le détail de l'erreur
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE CB_Erreur(sNomDuProfil est une chaine, sMessage est une chaine, sErreur est une chaîne)
      AjouteLigneDansStatus("Erreur lors du traitement de " + sNomDuProfil + " : " + sMessage);
      SI sErreur <> "" ALORS
      	AjouteLigneDansStatus("Détail de l'erreur: ")
      	AjouteLigneDansStatus(sErreur)
      FIN
